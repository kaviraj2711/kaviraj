#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num, remainder;
    
    
    scanf("%d", &num);
    
    remainder = num % 3;
    
    if (remainder == 0) {
        printf("The number is divisible by 3\n");
    } else {
        printf("The number is not divisible by 3 and gives a remainder %d\n", remainder);
    }
    
    return 0;
}
